  "scripts": {
    "build": "./bin/build",
    "clean": "rimraf .next",
    "db:migrate": "dotenv -c development prisma migrate dev",
    "db:push": "dotenv -c development prisma db push",
    "db:reset": "dotenv -c development prisma migrate reset",
    "db:reset:force": "dotenv -c development prisma migrate reset -- --force",
    "db:seed": "dotenv -c development prisma db seed -- --preview-feature",
    "db:studio": "dotenv -c development prisma studio",
    "dev": "next dev",
    "format": "prettier  --write .",
    "lint": "next lint",
    "setup": "./bin/setup",
    "setup:ci": "dotenv -c test prisma migrate reset -- --force",
    "start": "next start",
    "test": "jest",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "type-check": "tsc --pretty --noEmit"
  },
  "prisma": {
    "seed": "npm run ts-node prisma/seed.ts"
  },
